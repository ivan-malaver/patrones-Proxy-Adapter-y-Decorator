  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
